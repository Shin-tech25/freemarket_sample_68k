.header__mask__menu__nav-box__right__mask--mypage__popup
  .header__mask__menu__nav-box__right__mask--mypage__popup__iconimage
    .header__mask__menu__nav-box__right__mask--mypage__popup__iconimage__bgimage
      = image_tag("material/icon/member_photo_noimage_thumb.png", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__iconimage__bgimage__userimage')
    .header__mask__menu__nav-box__right__mask--mypage__popup__iconimage__inner
      .header__mask__menu__nav-box__right__mask--mypage__popup__iconimage__inner__name
        #{@user.nickname}
      .header__mask__menu__nav-box__right__mask--mypage__popup__iconimage__inner__count
        評価 0 出品数 0
  .header__mask__menu__nav-box__right__mask--mypage__popup__pointbox
    %ul.header__mask__menu__nav-box__right__mask--mypage__popup__pointbox__moneyandpoint
      %li
        %ul
          = link_to "#" ,class: 'header__mask__menu__nav-box__right__mask--mypage__popup__pointbox__moneyandpoint__item--money' do
            %li 売上金
            %li
              ¥0
              = icon("fa", "angle-right", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__pointbox__moneyandpoint__item--money__arrow')
      %li
        %ul
          = link_to "#" ,class: 'header__mask__menu__nav-box__right__mask--mypage__popup__pointbox__moneyandpoint__item--point' do
            %li ポイント
            %li
              P0
              = icon("fa", "angle-right", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__pointbox__moneyandpoint__item--point__arrow')
  %ul.header__mask__menu__nav-box__right__mask--mypage__popup__list
    = link_to "#", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item--head' do
      %li いいね！一覧
      = icon("fa", "angle-right", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item--head__arrow')
    = link_to "#", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item' do
      %li 出品した商品-出品中
      = icon("fa", "angle-right", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item__arrow')
    = link_to "#", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item' do
      %li 出品した商品-取引中
      = icon("fa", "angle-right", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item__arrow')
    = link_to "#", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item' do
      %li 出品した商品-売却済み
      = icon("fa", "angle-right", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item__arrow')
    = link_to "#", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item' do
      %li 購入した商品-取引中
      = icon("fa", "angle-right", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item__arrow')
    = link_to "#", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item' do
      %li 購入した商品-過去の取引
      = icon("fa", "angle-right", class: 'header__mask__menu__nav-box__right__mask--mypage__popup__list__item__arrow')